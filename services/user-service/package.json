{
  "name": "@services/user-service",
  "version": "1.0.0",
  "description": "User Service",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build || true",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "mkdir -p dist && ln -sf services/user-service/src/main.js dist/main.js 2>/dev/null || true && nest start --watch",
    "start:debug": "mkdir -p dist && ln -sf services/user-service/src/main.js dist/main.js 2>/dev/null || true && node --inspect=9229 -r ts-node/register -r tsconfig-paths/register ../../node_modules/@nestjs/cli/bin/nest.js start --watch",
    "start:prod": "node dist/main",
    "migration:create": "mikro-orm migration:create --config=src/scripts/mikro-orm.config.ts",
    "migration:up": "mikro-orm migration:up --config=src/scripts/mikro-orm.config.ts",
    "migration:down": "mikro-orm migration:down --config=src/scripts/mikro-orm.config.ts",
    "migration:pending": "mikro-orm migration:pending --config=src/scripts/mikro-orm.config.ts",
    "migration:list": "mikro-orm migration:list --config=src/scripts/mikro-orm.config.ts",
    "migration:fresh": "mikro-orm migration:fresh --config=src/scripts/mikro-orm.config.ts",
    "seed:default-user": "ts-node -r tsconfig-paths/register src/scripts/seed-default-user.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "@mikro-orm/core": "^6.1.1",
    "@mikro-orm/nestjs": "^6.1.1",
    "@mikro-orm/postgresql": "^6.1.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/platform-express": "^10.2.10",
    "@shared/infra": "*",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.5.9",
    "@mikro-orm/migrations": "^6.5.9",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
