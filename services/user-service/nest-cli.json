{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": false,
    "webpack": false,
    "tsConfigPath": "tsconfig.json",
    "assets": [
      {
        "include": "../proto/**/*",
        "outDir": "./dist"
      }
    ],
    "watchAssets": true,
    "builder": "tsc"
  }
}

